    "require": {
        "php": ">=7.4",
        "composer/installers": "~1.0",
        "guzzlehttp/guzzle": "^7.0",
        "monolog/monolog": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "AIImageGenerator\\": "src/"
        }
    },
    "license": "GPL-2.0-or-later",
    "config": {
